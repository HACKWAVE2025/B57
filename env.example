# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# EmailJS Configuration (for team invitations, todo reminders, and feedback)
VITE_EMAILJS_SERVICE_ID=your_emailjs_service_id
VITE_EMAILJS_TEMPLATE_ID=template_0tnir1s
VITE_EMAILJS_TEAM_INVITE_TEMPLATE_ID=template_0tnir1s
VITE_EMAILJS_TODO_TEMPLATE_ID=template_oi33v6o
VITE_EMAILJS_FEEDBACK_TEMPLATE_ID=template_00lom57
VITE_EMAILJS_PUBLIC_KEY=your_emailjs_public_key

# Google Drive API Configuration (for file backup)
VITE_GOOGLE_CLIENT_ID=your_google_oauth_client_id
VITE_GOOGLE_API_KEY=your_google_api_key

# Application Configuration
VITE_APP_NAME=Super Study App
VITE_APP_URL=http://localhost:3000
VITE_SUPPORT_EMAIL=support@yourdomain.com

# Feature Flags
VITE_ENABLE_EMAIL_INVITES=true

# Enhanced AI Features Configuration
# Google AI (Gemini) API Key for AI features
VITE_GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# VAPI Web Token for voice interviews
VITE_VAPI_WEB_TOKEN=your_vapi_web_token_here

# Optional: Custom Gemini Model (defaults to gemini-2.0-flash)
VITE_GEMINI_MODEL=gemini-2.0-flash

# Optional: Enable server-side OCR (requires backend setup)
VITE_ENABLE_SERVER_OCR=false

# Enhanced Features Configuration
# Set to 'true' to use the enhanced AI interview system with advanced features
VITE_USE_ENHANCED_APP=true

# Speech Analysis Configuration
VITE_SPEECH_ANALYSIS_ENABLED=true
VITE_FILLER_WORD_THRESHOLD=10
VITE_OPTIMAL_WPM_MIN=140
VITE_OPTIMAL_WPM_MAX=160

# Speech Transcription Configuration
VITE_TRANSCRIPTION_LANGUAGE=en-US
VITE_TRANSCRIPTION_CONFIDENCE_THRESHOLD=0.0
# 0.0 = Accept all results (best for accuracy)
# Higher values filter low-confidence results (may miss words)

# Body Language Analysis Configuration
VITE_BODY_LANGUAGE_ENABLED=true
VITE_EYE_CONTACT_OPTIMAL_MIN=60
VITE_EYE_CONTACT_OPTIMAL_MAX=80

# Performance Analytics Configuration
VITE_PERFORMANCE_TRACKING_ENABLED=true
VITE_HISTORICAL_DATA_RETENTION_DAYS=365

# Question Generation Configuration
VITE_INTELLIGENT_QUESTIONS_ENABLED=true
VITE_QUESTION_ADAPTATION_ENABLED=true
VITE_MAX_QUESTIONS_PER_INTERVIEW=10
VITE_ENABLE_FILE_SHARING=true
VITE_ENABLE_GOOGLE_BACKUP=true
VITE_ENABLE_TEAM_CHAT=true

# Development Settings
NODE_ENV=development
VITE_DEBUG_MODE=true

# Production Settings (uncomment for production)
# NODE_ENV=production
# VITE_DEBUG_MODE=false
# VITE_APP_URL=https://yourdomain.com


# Zoom Web SDK Configuration
# Get these from your Zoom Marketplace app
REACT_APP_ZOOM_SDK_KEY=your_zoom_sdk_key_here
ZOOM_SDK_KEY=your_zoom_sdk_key_here
ZOOM_SDK_SECRET=your_zoom_sdk_secret_here
